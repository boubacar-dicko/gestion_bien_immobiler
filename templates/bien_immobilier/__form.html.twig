

{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.nomBien) }}</div>
    <div class="col-md-2">{{ form_row(form.numeroBi) }}</div>
    <div class="col-md-4">{{ form_row(form.prix) }}</div>
</div>
<div class="row">
    <div class="col-md-2">{{ form_row(form.nombrePieces) }}</div>
    <div class="col-md-3">{{ form_row(form.sold) }}</div>
    <div class="col-md-2">{{ form_row(form.nombreEtage) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.ville) }}</div>
    <div class="col-md-2">{{ form_row(form.surface) }}</div>
</div>
<div class="row">
    <div class="col-md-10">{{ form_row(form.description) }}</div>
</div>
<button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}